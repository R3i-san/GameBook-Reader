<UserControl xmlns="https://github.com/avaloniaui"
             xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
             xmlns:d="http://schemas.microsoft.com/expression/blend/2008"
             xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006"
             mc:Ignorable="d" d:DesignWidth="800" d:DesignHeight="450"
             x:Class="GBReaderCremaL.Avalonia.Views.ReadView">
    <StackPanel>
        <TextBlock x:Name="PageText"></TextBlock>
        
        <WrapPanel x:Name="Choices" Orientation="Vertical">
            
        </WrapPanel>
        
        <Button x:Name="Home" Click="GoHome">Acceuil</Button>
        <Button x:Name="Reset" IsVisible="False" Click="Retry">Recommencer</Button>
        
    </StackPanel>
    
</UserControl>
